<% layout('./layouts/boilerplate') -%>
<body>
    <div class="row  row-cols-lg-4 row-cols-md-2 row-cols-sm-1 p-2">
        <%for(let listing of allListing){ %>
            <div class="card listing-card">
                <img class="card-img-top img-thumbnail" src="<%=listing.image.url  %>" alt="Listing Image">
                <div class="card-body">
                  <h5 class="card-title"><%=listing.title  %></h5>
                  <p>&#8377;<%=listing.price%></p>
                  <a href="/listings/<%=listing._id  %>" class="btn btn-primary bg-bnb">See Details</a>
                </div>
            </div>    
        <% } %>
    </div>        

</body>

